import material/material2tropic;

main() {
	manager = makeMaterialManager([]);
	setRendererType("html");
	content =
		MLines2(
			MTextButton("BUTTON", \ -> println("ON MBUTTON clicked"), [], []),
			MComponent(
				[MOnClick(\ -> println("ON MComponent clicked"))],
				\__ -> MText("COMPONENT", []) |> MBorderTopBottom(8.) |> MBorderLeftRight(8.)
			)
		);
	mrender(manager, false, content);
}