import material/material_ui;

main() {
	manager = makeMaterialManager([]);
	setRendererType("html");

	leftTop = make(Point(300., 100.));
	fpprint("leftTop")(leftTop);
	size = make(WidthHeight(200., 200.));
	fpprint("size")(size);

	content = MGroup2(
		MFrame(0., 0., [MFill(MBlue(200))], TFillXY()),
		MEditableRectangle(leftTop, size, [MTopLeft(), MTopRight()], [
			// MCustomHandle(\h -> TRectangle([MFill(if (h == MTopLeft()) MRed(500) else MBlack())], TFixed(20., 10.))),
			MResizableEnabled(const(true), const(false))
		])
	);

	mrender(manager, true, content);
}