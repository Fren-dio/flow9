import material/material_ui;

main() {
	manager = makeMaterialManager([]);
	setRendererType("html");

	content = 
		MLines([
			MText("Flow header text", []),
			MTextInput(make("I am text input"), [], []),
			MTextButton("BUTTON", \ -> println("click"), [], []),
			MCheckBox(MText("Checkbox", []), make(false), [])
		])
		|> (\m -> MGroup2(
			MFrame(0., 0., [MStroke(MBlue(500))], TFillXY()),
			MBorder4(8., m)
		));

	mrenderTo(manager, true, content, "flow_root");
}