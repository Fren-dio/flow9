import fabric/env;
import fabric/wrappers;
import fabric/frp_typed;
import fabric/fabric2material;
import material/material2tropic;

export {
	brender(f : Fabric) -> () -> void;
}

brender(f : Fabric) -> () -> void {
	setRendererType("html");
	env = makeFabricEnv();
	mrender(makeMaterialManager([]), true, MFabric(env, f));
}