import fabric/env;
import fabric/wrappers;
import fabric/fabric2material;
import material/material2tropic;

export {
	brender(f : Fabric) -> () -> void;
}

brender(f : Fabric) -> () -> void {
	env = makeFabricEnv();
	fallback = fabric2material(env, f);
	mrender(makeMaterialManager([]), true, fallback);
}