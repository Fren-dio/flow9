native map : ([?], (?) -> ??) -> [??] = Native.map;
native println2 : io (flow) -> void = Native.println;
native i2s : (int) -> string = Native.i2s;
i2s(i) { cast(i : int -> string); }

Struct(val : int);
Struct2(val : string);

main() {
	println2("Create init array");
	arr = [Struct(1), Struct(2), Struct(3)];
	println2("\nCreate array by map()");
	arr2 = map(arr, \v -> Struct2(i2s(-v.val)));
	println2("\nPrint new array");
	println2(arr2);
	// println2("Print init array");
	// println2(arr1);
	println2("\nEND");
}
