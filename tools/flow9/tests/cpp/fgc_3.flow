native map : ([?], (?) -> ??) -> [??] = Native.map;
native println2 : io (flow) -> void = Native.println;

Struct(val : int);

main() {
	val = 5;

	println2("Create init array");
	arr = [Struct(1), Struct(2), Struct(3)];
	println2("\nCreate array by map()");
	arr2 = map(arr, \v -> val);
	println2("\nPrint new array");
	println2(arr2);
	
	valRef = ref 6;
	println2("\nCreate array by map() from Ref");
	arr3 = map(arr, \v -> valRef); // [5, 5, 5]
	valRef := 7;

	println2("\nPrint new array");
	println2(arr3); // [ref 7, ref 7, ref 7]
	
	println2("\nEND");
}
